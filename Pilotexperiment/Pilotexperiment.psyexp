      <CodeComponent name="setup_mask">
        <Param name="Begin Experiment" updates="constant" val="class Mask:&amp;#10;    def __init__(self,N,M,noise_num,noise_size):&amp;#10;        self.N=N&amp;#10;        self.M=M&amp;#10;        self.noise_num=noise_num&amp;#10;        self.noise_size=noise_size&amp;#10;        self.mask = np.zeros((N,M))&amp;#10;    def getEmptyMask(self):&amp;#10;        return np.ones( (self.N,self.M))&amp;#10;    def getMask(self):&amp;#10;        N_m=np.random.randint(self.N, size=self.noise_num)&amp;#10;        M_m=np.random.randint(self.M, size=self.noise_num)&amp;#10;        mask = np.ones( (self.N,self.M))&amp;#10;        for k in xrange(self.noise_num):&amp;#10;                noise_s=np.random.normal(self.noise_size, 0.25*self.noise_size,1)&amp;#10;                x_l=int(N_m[k]-noise_s/2) if int(N_m[k]-noise_s/2)&gt;0 else 0&amp;#10;                y_b=int(M_m[k]-noise_s/2) if int(M_m[k]-noise_s/2)&gt;0 else 0&amp;#10;                x_r=int(N_m[k]+noise_s/2) if int(N_m[k]+noise_s/2)&lt;self.N else self.N&amp;#10;                y_t=int(M_m[k]+noise_s/2) if int(M_m[k]+noise_s/2)&lt;self.M else self.M&amp;#10;                for i in xrange(x_l,x_r ):&amp;#10;                    for j in xrange(y_b,y_t):&amp;#10;                        mask[i,j] = -1&amp;#10;        return mask" valType="extendedCode"/>
        <Param name="name" updates="None" val="setup_mask" valType="code"/>
